-- Scripts:

local function MZKT_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	loadstring(game:HttpGet('https://raw.githubusercontent.com/gooserblx/Quix/main/Universal'))()
end
coroutine.wrap(MZKT_fake_script)()
