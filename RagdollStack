local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ragdoll Stack! - V1.1 - QuixHub", "Ocean")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
local Universal = Window:NewTab("Universal")
local UniversalSection = Universal:NewSection("Universal")
local TP = Window:NewTab("Teleports")
local TPSection = TP:NewSection("Teleports")



MainSection:NewSlider("Walk Speed", "Changes your walk speed.", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MainSection:NewLabel("Visuals")

MainSection:NewTextBox("Fake Wins Adder", "Will add fake wins to your win count. Will not show to anyone else", function(txt)
	print(txt, "fake wins added")
    game.Players.LocalPlayer.leaderstats.Wins.Value += txt
end)

MainSection:NewTextBox("Fake Ragdolls Adder", "Will add fake ragdolls to your win count. Will not show to anyone else", function(txt)
	print(txt, "fake ragdolls added")
    game.Players.LocalPlayer.leaderstats.Ragdolls.Value += txt
end)

MainSection:NewLabel("Farms")



UniversalSection:NewSlider("Walk Speed", "Will change your walkspeed.", 500, 1, function(s) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
	end)
	UniversalSection:NewSlider("Jump Power", "Will change your JumpPower.", 500, 1, function(j) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
	end)
	_G.infinjump = false

	local Player = game:GetService("Players").LocalPlayer
	local Mouse = Player:GetMouse()
	Mouse.KeyDown:connect(function(k)
		if _G.infinjump then
			if k:byte() == 32 then
				local Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
				Humanoid:ChangeState("Jumping")
				wait(0.1)
				Humanoid:ChangeState("Seated")
			end
		end
	end)
	
	UniversalSection:NewToggle("Infinite Jump", "Lets you fly by jumping.", function(state)
		if state then
			_G.infinjump = true
		else
			_G.infinjump = false
		end
	end)
TPSection:NewButton("The First Walls End TP", "Teleports you to the end of The First Walls level.", function()
    local pos = game.Players.LocalPlayer.Character.HumanoidRootPart
    print("The First Walls TP Complete!")
    pos.CFrame = CFrame.new(16.7769508, 22.0335026, -257.314819, -0.305279166, 0.497483373, -0.811982095, -6.41613696e-09, 0.852686882, 0.522422314, 0.952262878, 0.159484655, -0.26030755)
end)

